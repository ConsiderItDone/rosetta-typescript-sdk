    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.3.2",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4",
    "uglifyjs": "^2.4.11"
  },
  "repository": {
